<!-- Loaded by marknotes so images can be displayed in a modal form -->
<div class="modal fade" id="Modal_ImgGallery" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
	<div class="modal-dialog" style="max-width:65%;max-height:65%;">
		<div class="modal-content">
			<div class="modal-body">
				<h3 id="imgTitle"></h3>
				<img id="imgGallery" src="" />
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-primary" id="btnImgGalleryClose">%CLOSE%</button>
			</div>
		</div>
	</div>
</div>
